<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="logo">
            <arguments>
                <argument name="logo_file" xsi:type="string">images/logo.jpg</argument>
                <argument name="logo_img_width" xsi:type="number">201</argument> 
                <argument name="logo_img_height" xsi:type="number">71</argument>
            </arguments>
        </referenceBlock>
        <referenceContainer name="footer">

            <block class="Magento\Cms\Block\Block" after="footer_links"  name="footer-social-icons">
                <arguments>
                    <argument name="block_id" xsi:type="string">footer-social-icons</argument>
                </arguments>
            </block>   
            <block class="Magento\Store\Block\Switcher" name="" as="store_switcher" template="switch/stores.phtml"/>            
        </referenceContainer>       
    </body>
</page>
